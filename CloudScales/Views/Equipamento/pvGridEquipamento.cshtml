@model List<EquipamentoViewModel>


<table class="table table-responsive text-white">
    <tr>
        <th>CaminhaoID</th>
        <th>QtdBalanca</th>
    </tr>
    @foreach (var equip in Model)
    {
        <tr>
            <td>@equip.CaminhaoID</td>
            <td>@equip.QtdBalanca</td>
        </tr>
    }
    @if (Model.Count() == 0)
    {
        <tr>
            <td>
                Nenhum registro encontrado.
            </td>
        </tr>
    }
</table>